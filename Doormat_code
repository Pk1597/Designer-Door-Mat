#Get the input of N and M
N, M = map(int,input().split())
ch='.|.'
text="WELCOME"
#Initializing for loop for upper half
for i in range(1,N,2): 
    a= (ch*i).center(M,'-')
    print(a)
b= text.center(M,'-')
print(b)
#Initializing for loop for lower half
for i in range(N-2,-1,-2): 
    a= (ch*i).center(M,'-')
    print(a)
